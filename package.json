{
  "name": "chat-pd-poa",
  "version": "1.0.0",
  "description": "Chat PD POA - Sistema de Consulta Urbanística de Porto Alegre",
  "private": true,
  "scripts": {
    "dev": "npm run dev:frontend",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:backend": "cd backend/supabase && npx supabase start",
    "dev:all": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "build": "cd frontend && npm run build",
    "install:all": "npm install && cd frontend && npm install",
    "deploy:functions": "cd backend/supabase && npx supabase functions deploy",
    "deploy:all": "npm run build && npm run deploy:functions",
    "test": "cd tests && npm test",
    "clean": "rm -rf node_modules frontend/node_modules frontend/dist"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/usuario/chat-pd-poa-07.git"
  },
  "keywords": [
    "urbanismo",
    "porto-alegre",
    "consulta",
    "legislação",
    "supabase",
    "react"
  ],
  "author": "Aurora",
  "license": "MIT"
}